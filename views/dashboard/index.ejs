<% include ../header %>

<main class='main-first-page' ng-controller='DashboardCtrl'>
  <div class='container'>
    <h1 id='welcome'>Welcome!
      <%if(bannerPath) { %>
        <div class='col-xs-12 col-sm-12 col-md-12'>
          <img src='<%= bannerPath %>' alt="" />
        </div>
      <% } %>
    </h1>

    <% include _navTabs %>

    <div class='container border-blue'>
      <div class='row'>
        <div class='col-xs-12 col-sm-12 col-md-12'>
          <% include _buttons %>
        </div>
        <div class='col-xs-12 col-sm-12 col-md-12'>
          <div class='row'>
            <div class='dashboard-index'>
              <div class='col-md-2'></div>
              <div class='col-md-8'>
                Here you can manage your Account Profile by adding more Chat Sessions and Account Managers, or changing your Payment and Contact Details or Password.
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>

<% include ../footer %>
