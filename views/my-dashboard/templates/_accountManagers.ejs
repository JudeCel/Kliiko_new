<div ng-if='myDashboard.isTabActive("accountManager")'>
  <h4 class='text-center'>Please select an <strong>Account</strong></h4>

  <table class='table table-responsive table-dashboardElements'>
    <thead>
      <tr>
        <th>Account Name</th>
        <th class='pull-right'>Select Account Hub</th>
      </tr>
    </thead>
    <tbody ng-repeat='au in myDashboard.accountUsers.accountManager.data'>
      <tr>
        <td>{{ au.Account.name }}</td>
        <td>
          <a href='{{ au.dashboardUrl }}' class='go-to-dashboard glyphicon glyphicon-play pull-right' title='Go to Account Hub' />
        </td>
      </tr>
    </tbody>
  </table>
</div>
